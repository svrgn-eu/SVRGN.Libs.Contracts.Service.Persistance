<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<LangVersion>latest</LangVersion>
		<Version>1.0.0-beta01</Version>
		<Authors>SVRGN.eu</Authors>
		<Copyright>2023 SVRGN.eu</Copyright>
		<Description>Interface for persistance services used by SVRGN service implementations.</Description>
		<PackageReleaseNotes>Removed IOWrapper as double entry</PackageReleaseNotes>
		<PackageTags>Services;Interfaces;SVRGN;Persistance;</PackageTags>
		<PackageProjectUrl>https://SVRGN.eu</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="SVRGN.Libs.Contracts.Base" Version="1.0.0-alpha19" />
	  <PackageReference Include="SVRGN.Libs.Contracts.Base.IO" Version="1.0.0-beta01" />
	</ItemGroup>

</Project>
